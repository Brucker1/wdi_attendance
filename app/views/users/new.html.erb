

<div class = "logout">
  <%= button_to "Logout", '/sessions', method: "delete" %>
  <br /><%= button_to "Back", :back %><br />
</div>

<h1>Register:</h1>

<%= form_for @user do |f| %>
  <%= f.label :name, "Name" %>:
  <%= f.text_field :name %><br>
  <%= f.label :email, "Email" %>:
  <%= f.text_field :email %><br>
  <%= f.label :password, "Password" %>:
  <%= f.password_field :password %><br>
  <%= f.submit %>
<% end %>
